  "scripts": {
    "build": "node ./scripts/systemjs-mapping.js && npm run changelog:generate",
    "build:demo": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng build --app demo --prod --aot true --base-href /oden/demo/",
    "build:docs": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng build --app docs --aot false --base-href /oden/docs/",
    "changelog:add": "node ./scripts/changelog/add.js",
    "changelog:generate": "node ./scripts/changelog/generate.js",
    "e2e": "ng e2e --no-webdriver-update --suit=versions,wizard,overview,billSpecifications,scheduleDefinitions,focusShift,externalLink,taskSearch,validationsummary,bucket,relationGraph,odenForm,library,blocktree",
    "e2e:automation": "./e2e/protractor_automation.sh",
    "e2e:firefox": "protractor ./config/protractor.conf.js --browser=firefox --multiCapabilities",
    "e2e:suite": "protractor ./config/protractor.conf.js",
    "e2e:watch": "watch \"npm run e2e\" src e2e --wait=1",
    "lint": "tslint \"src/**/*.ts\" && tslint \"e2e/**/*.ts\"",
    "postbuild:docs": "mv ./src/docs-app/docs/* ./dist",
    "posttest": "npm run e2e",
    "prebuild:docs": "node ./scripts/readmes-to-json.js",
    "pree2e": "webdriver-manager update --gecko false --versions.chrome=2.43",
    "preinstall": "npm prune",
    "prelint": "node ./node_modules/typescript/bin/tsc -p ./tslint/custom-rules/tsconfig.json",
    "prestart:docs": "node ./scripts/readmes-to-json.js",
    "pretest": "npm run lint && npm run smoketest",
    "pretest:watch": "npm run lint",
    "smoketest": "npm run build:demo && chmod +x scripts/smoke-test.sh && sh ./scripts/smoke-test.sh",
    "start": "ng serve --app demo",
    "start:demo": "npm run start",
    "start:docs": "ng serve --app docs",
    "start:e2e": "npm run start -- --env=e2e",
    "start:external": "http-server -S -C config/cert.pem -K config/key.pem -p 4201 src/demo-app/demo-app-external",
    "start:hmr": "ng serve --app demo --hmr -e hmr",
    "start:prod": "npm run start -- --app demo --prod --aot true",
    "start:ssl": "npm run start -- --ssl",
    "test": "ng test --watch=false --code-coverage",
    "test:watch": "ng test"
  },
  "version": "1.13.0"
